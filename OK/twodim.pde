class twodim{
  float a;
  float b;
  float c;
  String as;
  String bs;
  String cs;
  int scale;
  
  twodim(float a, float b, float c, String as, String bs, String cs){
    set(a,b,c,as,bs,cs);
  }
  
  void set(float a, float b, float c, String as, String bs, String cs){
    this.a = a;
    this.b = b;
    this.c = c;
    this.as = as;
